
public double factor(ArrayList<Double> list)
    {
        //storing the number in integer type "num"
        int num= list.get(0).intValue();
        if(num==1||num==2)
        {
            // 2 have only 1 as a factor
            System.out.println("1");
            return 0;
        }
        System.out.println();
        for(int i=2;i<num;i++)
        {
            //iterate from 2 to the number
            if(num%i==0)
            // if the number is completely divisible, then it is a factor
            System.out.print(i+" ");
        }        
        return 0;
    }
	